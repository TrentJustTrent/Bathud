{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "lib": ["ES2020", "DOM"],
    "esModuleInterop": true,
    "moduleResolution": "node",
    "strict": true,
    "outDir": "scripts/build",
    "rootDir": ".",
    "resolveJsonModule": true,
    "skipLibCheck": true
  },
  "include": ["scripts/**/*", "ags/widget/utils/config/configSchema.ts"]
}